[color]
  ui = auto
[alias]
	st = status
	co = checkout
	ca = commit -a
	ps = push
	unstage = reset HEAD
	lg = log --stat --graph --decorate=full
	pl = pull
[push]
  default = simple
[filter "lfs"]
	required = true
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
[ghq]
	root = ~/src
